{
  "name": "carno-php/consul",
  "license": "MIT",
  "require": {
    "carno-php/coroutine": "^1.0",
    "carno-php/promise": "^1.0",
    "carno-php/channel": "^1.0",
    "carno-php/timer": "^1.0",
    "carno-php/http": "^1.0",
    "carno-php/net": "^1.0",
    "carno-php/log-slim": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Carno\\Consul\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Carno\\Consul\\Tests\\": "tests"
    },
    "files": [
      "tests/prepare.php"
    ]
  },
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
